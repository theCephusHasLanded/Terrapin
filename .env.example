# API Configuration
# Set to your Spring Boot backend URL
REACT_APP_API_URL=http://localhost:8080/api

# Set to 'true' to use mock data instead of real API calls
# Useful for frontend development without a running backend
REACT_APP_USE_MOCK_DATA=false

# Stripe API Configuration for payments
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_test_key_here

# Environment (development, test, production)
REACT_APP_ENV=development

# Debug mode
REACT_APP_DEBUG=true

# Database Configuration (for Spring Boot backend)
# Use this in terrapin-backend/.env
DB_URL=jdbc:postgresql://localhost:5432/terrapin
DB_USERNAME=postgres
DB_PASSWORD=your_database_password
